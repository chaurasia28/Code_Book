//deque is implemented by array pointers
#include<iostream>
#include<deque>
using namespace std;
int main()
{
    deque<int>dq;
    dq.push_front(20);
    dq.push_back(30);
    dq.push_front(10);
    cout<<dq.front()<<endl;//output is 10
    dq.pop_front();
    cout<<dq.front()<<endl;//output is 20
}