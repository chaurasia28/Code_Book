#include<iostream>
#include<queue>
using namespace std;
int main()
{
    queue<int>q;
    int n;
    cin>>n;
    cout<<"Enter the elements in queue"<<endl;
    for(int i=0;i<n;i++)
    {
        int data;
        cin>>data;
        q.push(data);
    }
    cout<<endl;
    cout<<"Elements in queue"<<endl;
    for(int i=0;i<n;i++)
    {
        cout<<q.front()<<" ";
        q.pop();
    }
    cout<<endl;
    cout<<q.back()<<" ";
    cout<<endl;
    cout<<"Front element in queue is"<<q.front()<<endl;
    cout<<"Size of queue is"<<q.size()<<endl;
    if(q.empty())
    {
        cout<<"Queue is empty"<<endl;
    }
    else
    {
        cout<<"Queue is not empty"<<endl;
    }
    
}