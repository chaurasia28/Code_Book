#include<iostream>
using namespace std;
int sum_array(int arr[],int n)
{
    int sum=0;
    for(int i=0;i<n;i++)
    {
        sum=sum+arr[i];
    }
    return sum;
}
int main()
{
    int n,arr[100];
    cout<<"Enter size of an array:";
    cin>>n;
    cout<<"Enter elements of an array:";
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
   int result= sum_array(arr,n);
   cout<<"Sum of array:"<<result<<endl;
}